		"add:component": "bun dlx shadcn@latest add --yes --cwd ."
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.7",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@repo/ui-utils": "workspace:*",
		"class-variance-authority": "^0.7.0",
		"lucide-react": "^0.441.0",
		"react": "^18.2.0"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.2.0"
	},
	"publishConfig": {
		"access": "public"
	}
}