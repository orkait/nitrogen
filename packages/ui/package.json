{
	"name": "@repo/ui",
	"version": "0.0.0",
	"license": "MIT",
	"sideEffects": [
		"**/*.css"
	],
	"type": "module",
	"exports": {
		"./styles": "./src/styles.css",
		"./placeholder": {
			"types": "./dist/placeholder.d.ts",
			"import": "./dist/placeholder.js",
			"require": "./dist/placeholder.cjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf .turbo && rimraf node_modules && rimraf dist",
