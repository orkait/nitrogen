		"@repo/ui": "workspace:*",
		"@repo/ui-utils": "workspace:*",
		"react": "^18.2.0"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^19.2.4"
	},
	"publishConfig": {
		"access": "public"
	}
